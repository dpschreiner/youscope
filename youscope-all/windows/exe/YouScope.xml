<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar>youscope-starter.jar</jar>
  <outfile>YouScope.exe</outfile>
  <errTitle>YouScope</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl>http://www.youscope.org</supportUrl>
  <customProcName>true</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>YouScope.ico</icon>
  <var>PATH=%CD%drivers;%PATH%</var>
  <classPath>
    <mainClass>org.youscope.starter.Starter</mainClass>
    <cp>youscope-starter.jar</cp>
  </classPath>
  <jre>
    <path></path>
    <minVersion>1.6.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <initialHeapSize>256</initialHeapSize>
    <initialHeapPercent>10</initialHeapPercent>
    <maxHeapSize>1024</maxHeapSize>
    <maxHeapPercent>50</maxHeapPercent>
  </jre>
  <splash>
    <file>splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
  <versionInfo>
    <fileVersion>1.0.0.0</fileVersion>
    <txtFileVersion>1.0, compatible MicroManager 1.4</txtFileVersion>
    <fileDescription>YouScope - Microscope Control Environment</fileDescription>
    <copyright>Moritz Lang, ETH Zurich, Switzerland</copyright>
    <productVersion>1.0.0.0</productVersion>
    <txtProductVersion>1.0, compatible MicroManager 1.4</txtProductVersion>
    <productName>YouScope - Microscope Control Environment</productName>
    <companyName>ETH Zurich, Switzerland</companyName>
    <internalName>YouScope</internalName>
    <originalFilename>YouScope.exe</originalFilename>
  </versionInfo>
</launch4jConfig>